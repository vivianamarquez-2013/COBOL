CREATE TABLE LINEAS(
CODIGO_LN INTEGER PRIMARY KEY,
NOMBRE_LN VARCHAR2(30)
);

CREATE TABLE PROV(
CODIGO_PR INTEGER PRIMARY KEY,
NOMBRE_PR VARCHAR2(30)
);

CREATE TABLE TIPOIVA(
CODIGO_TI INTEGER PRIMARY KEY,
NOMBRE_PR VARCHAR2(30),
VALOR_TI INTEGER
);

*-----

CREATE TABLE PRODUCTO(
CODIGO_PD INTEGER PRIMARY KEY,
NOMBRE_PD VARCHAR2(30),
LINEA_PD INTEGER,
PROVEEDOR_PD INTEGER,
TIPO_IVA_PD INTEGER
);
